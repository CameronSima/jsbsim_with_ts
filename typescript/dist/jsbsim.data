  </switch>

  <pid name="fcs/etc/throttle-controller-pid[0]">
   <input>fcs/etc/propeller-speed-error-switch[0]</input>
   <kp> 0.0 </kp>
   <ki> 0.0002 </ki>
   <trigger>fcs/etc/windup-trigger[0]</trigger>
  </pid>

  <actuator name="fcs/etc/throttle-actuator[0]">
