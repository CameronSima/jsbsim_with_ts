            <!-- In situations where TVC control is not called for, disable TVC SS Trim if manuevering-->
            <fcs_function name="fcs/ss-trim-override">
                <function>
                    <table>
                        <independentVar lookup="row">velocities/q-aero-rad_sec</independentVar>
                        <tableData>
                            -0.15  0
                            -0.1   1
                             0.0   1
                             0.1   1
                             0.15  0
