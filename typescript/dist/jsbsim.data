        <scheduled_gain name="indicated-fuel2-kg">
            <input>/consumables/fuel/tank[1]/level-lb</input>
            <gain>0.4536</gain>
            <table>
                <independentVar>/instrumentation/engine/eng-n-koef</independentVar>
                <tableData>
