   </description>
   <function>
    <sum>
     <property>hydro/pitch-deg</property>
     <product>
      <value>1.0</value>
      <max>
       <value>-30.0</value>
       <min>
        <property>hydro/orientation/wave-pitch-trim-change-deg</property>
        <value>30.0</value>
       </min>
      </max>
     </product>
    </sum>
   </function>
