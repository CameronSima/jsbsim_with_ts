            <output>/systems/l410/battery-all-ok</output>
        </switch>

        <switch name="battery-one-ok">
            <default value="0.0000"/>
            <test logic="OR" value="1.0000">
                /systems/l410/battery1-ok == 1
                /systems/l410/battery2-ok == 1
