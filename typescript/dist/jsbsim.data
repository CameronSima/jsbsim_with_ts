<!--
  File:     Pterosaur.xml
  Inputs:
    name:          Pterosaur
    type:          glider
    max weight:   352.02 lb
    wing span:     36.75 ft
    length:        32.81 ft
    wing area:     47.36 sq-ft

    gear type:     taildragger
