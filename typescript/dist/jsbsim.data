        </tableData>
       </table>
    </product>
   </function>
   <function name="aero/coefficient/CDmach">
    <description>Drag_due_to_mach</description>
    <product>
     <property>aero/qbar-psf</property>
     <property>metrics/Sw-sqft</property>
       <table>
        <independentVar>velocities/mach</independentVar>
        <tableData>
         0.0000  0.0000
         0.8100  0.0000
         1.1000  0.0230
         1.8000  0.0150
        </tableData>
       </table>
    </product>
   </function>
   <function name="aero/coefficient/CDDlef">
    <description>Drag_due_to_leading_edge_flap_deflection</description>
    <product>
     <property>aero/qbar-psf</property>
     <property>metrics/Sw-sqft</property>
     <property>fcs/lef-pos-rad</property>
       <table>
        <independentVar>aero/alpha-rad</independentVar>
        <tableData>
         -0.1750  0.0030
         -0.0870  0.0010
          0.0000  0.0000
          0.0870  0.0010
          0.1750  0.0020
          0.2620  0.0040
          0.3490  0.0070
          0.4360  0.0110
          0.5240  0.0150
          0.6110  0.0190
          0.6980  0.0230
          0.7850  0.0240
        </tableData>
       </table>
    </product>
   </function>
   <function name="aero/coefficient/CDDflaps">
    <description>Drag_due_to_trailing_edge_flaps</description>
    <product>
     <property>aero/qbar-psf</property>
     <property>metrics/Sw-sqft</property>
     <property>fcs/flaperon-mix-rad</property>
     <value>0.0800</value>
    </product>
   </function>
   <function name="aero/coefficient/CDgear">
    <description>Drag_due_to_gear</description>
    <product>
     <property>aero/qbar-psf</property>
     <property>metrics/Sw-sqft</property>
     <property>gear/gear-pos-norm</property>
     <value>0.0270</value>
    </product>
   </function>
   <function name="aero/coefficient/CDDsb">
    <description>Drag_due_to_speedbrake_deflection</description>
    <product>
     <property>aero/qbar-psf</property>
     <property>metrics/Sw-sqft</property>
     <property>fcs/speedbrake-pos-rad</property>
       <table>
        <independentVar>aero/alpha-rad</independentVar>
        <tableData>
         -0.1750 -0.0545
         -0.0870 -0.0225
          0.0000  0.0096
