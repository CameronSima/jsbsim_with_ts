   <function>
    <quotient>
     <property> hydro/v-fps </property>
     <pow>
      <product>
       <value> 32.185039 </value>
       <property>hydro/hull-beam-ft</property>
      </product>
      <value> 0.5 </value>
     </pow>
    </quotient>
   </function>
  </fcs_function> 

