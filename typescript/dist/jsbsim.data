                <product>
                    <property>aero/qbar-psf</property>
                    <property>metrics/Sw-sqft</property>
                    <value>0.000</value>
                </product>
            </function>
        </axis>
    </aerodynamics>

    <output name="BallOut.csv" type="CSV" rate="1">
