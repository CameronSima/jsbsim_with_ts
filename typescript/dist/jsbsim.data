   <switch name="Running">
     <default value="0"/>
     <test logic="AND" value="systems/legs/running-force">
        gear/unit[1]/WOW eq 1
        gear/unit[2]/WOW eq 1
        fcs/throttle-pos-norm gt 0
     </test>
     <output>external_reactions/legs/magnitude</output>
   </switch>

  </channel>

</system>
