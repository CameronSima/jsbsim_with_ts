     </le>
     <property>fcs/mixture/altitude-standard-pos</property>
     <sum>
      <value>1.0</value>
      <product>
       <property>fcs/mixture/manual-control-factor</property>
       <difference>
        <property>fcs/mixture-cmd-norm[0]</property>
        <value>0.5</value>
       </difference>
      </product>
     </sum>
    </product>
   </function>
   <clipto>
    <min>0.0</min>
    <max>1.0</max>
