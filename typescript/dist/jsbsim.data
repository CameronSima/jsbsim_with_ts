                <input>fcs/yaw-trim-sum</input>
                <gain>0.01745</gain>
                <range>
                    <min>-27</min>
                    <max>27</max>
                </range>
                <output>fcs/rudder-pos-rad</output>
            </aerosurface_scale>
        </channel>
        <channel name="Flaps">
            <kinematic name="Flaps Control">
                <input>fcs/flap-cmd-norm</input>
                <traverse>
                    <setting>
                        <position>0</position>
                        <time>0</time>
                    </setting>
                    <setting>
                        <position>15</position>
                        <time>2</time>
                    </setting>
                    <setting>
                        <position>25</position>
                        <time>1</time>
                    </setting>
                    <setting>
                        <position>40</position>
